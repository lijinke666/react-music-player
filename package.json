{
  "name": "react-jinke-music-player",
  "version": "3.1.0",
  "description": "Maybe must a beautiful and responsive react music player component",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run demo",
    "test": "jest --no-cache",
    "clean": "rimraf lib && rimraf assets",
    "auto": "postcss -u autoprefixer -c postcss.config.json --no-map -o assets/index.css  assets/index.css",
    "buildcss": "lessc src/index.less assets/index.css && npm run auto",
    "buildjs": "babel src -d lib",
    "pub": "npm run clean && npm run buildjs && npm run buildcss && npm run buildcss",
    "build-demo": "webpack --env.mode=PROD --progress --config  ./example/webpack.config.js",
    "demo": "webpack-dev-server --progress --inline --hot --config  ./example/webpack.config.js",
    "prepublish": "npm run pub"
  },
  "author": "jinke.li",
  "repository": {
    "type": "git",
